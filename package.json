{
  "name": "talabheim",
  "version": "1.0.0",
  "description": "Custom Warhammer Fantasy Ruleset support tool",
  "scripts": {
    "clean": "rimraf dist && rimraf .cache",
    "build": "rimraf dist && parcel build src/index.html --public-url .",
    "dev": "rimraf dist && parcel src/index.html",
    "start": "npm run dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isthisstackoverflow/talabheim.git"
  },
  "keywords": [
    "warhammer fantasy",
    "custom tool"
  ],
  "author": "Sen <superfastblitzmann@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isthisstackoverflow/talabheim/issues"
  },
  "homepage": "https://github.com/isthisstackoverflow/talabheim#readme",
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "parcel": "^1.12.3",
    "rimraf": "^2.6.3"
  }
}
